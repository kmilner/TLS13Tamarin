                      <'clientats', 
                       h(<<<p_messages, '11', '0', 'ce', pk(~ltkS)>, '15', signature>, 
                          '20', verify_data>)
                      >,
                      '32'),
               Expand(p_ms,
                      <'serverats', 
                       h(<<<p_messages, '11', '0', 'ce', pk(~ltkS)>, '15', signature>, 
                          '20', verify_data>)
                      >,
                      '32'),
               p_hs_keyc, p_hs_keys, <'0', 'auth'>,
               Expand(p_ms,
                      <'ems', 
                       h(<<<p_messages, '11', '0', 'ce', pk(~ltkS)>, '15', signature>, 
                          '20', verify_data>)
                      >,
                      '32'),
               p_rms, p_cert_req
   ),
   RecvStream( ~tid, $C, $S, <'0', 'auth'>,
               Expand(Expand(p_ms,
                             <'serverats', 
                              h(<<<p_messages, '11', '0', 'ce', pk(~ltkS)>, '15', signature>, 
                                 '20', verify_data>)
                             >,
                             '32'),
                      <'adke_wk', '0'>, '32')
   )
   ]

